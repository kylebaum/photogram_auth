<div class="page-header">
  <h1>
    My likes
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Likes</th>
        <th>Username</th>
      </tr>

      <% @my_likes.each do |like| %>
      <tr>
        <td><%= like.id %></td>
        <td>
          <%= like.user.username %>

        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
