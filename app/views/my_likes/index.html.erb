<div class="page-header">
  <h1>
    My likes
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @my_likes.each do |like| %>
      <tr>
        <td><%= like.id %></td>
        <td>
          <a href="/users/<%= user.id %>" class="btn btn-primary">Show</a>

        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
